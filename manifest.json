{
    "manifest_version": 2,
    "name": "Send to API",
    "version": "1.0",
    "description": "A Chrome extension that allows you to send selected text to an API",
    "permissions": [
        "contextMenus",
        "storage",
        "http://hcbille.se/*",
        "activeTab"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Send to API"
    }
}
